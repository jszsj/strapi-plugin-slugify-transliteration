{
	"$schema": "https://json.schemastore.org/package",
	"name": "strapi-plugin-slugify-transliteration",
	"version": "0.1.1",
	"description": "A plugin for Strapi Headless CMS that provides the ability to auto slugify a field for any content type.",
	"scripts": {
		"lint": "eslint . --fix",
		"format": "prettier --write **/*.{ts,js,json,yml}"
	},
	"author": {
		"name": "@jszsj",
		"url": "https://github.com/jszsj"
	},
	"maintainers": [
		{
			"name": "@jszsj",
			"url": "https://github.com/jszsj"
		}
	],
	"homepage": "https://github.com/jszsj/strapi-plugin-slugify-transliteration#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/jszsj/strapi-plugin-slugify-transliteration.git"
	},
	"bugs": {
		"url": "https://github.com/jszsj/strapi-plugin-slugify-transliteration/issues"
	},
	"dependencies": {
		"@sindresorhus/slugify": "1.1.0",
    "transliteration": "^2.3.5"
	},
	"devDependencies": {
		"eslint": "^8.8.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-node": "^11.1.0",
		"prettier": "^2.5.1"
	},
	"peerDependencies": {
		"@strapi/strapi": "^4.0.7",
		"@strapi/utils": "^4.0.7",
		"lodash": "^4.17.21",
		"yup": "^0.32.9"
	},
	"strapi": {
		"displayName": "Slugify",
