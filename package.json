		"kind": "plugin"
	},
	"engines": {
		"node": ">=14.19.1 <=18.x.x",
		"npm": ">=6.0.0"
	},
	"keywords": [
		"strapi",
		"strapi-plugin",
		"plugin",
		"strapi plugin",
		"slug",
		"slugify"
	],
	"license": "MIT"
}
